<!-- masonry
  ================================================== -->
<section id="bricks">

    <div class="row masonry">

        <!-- brick-wrapper -->
        <div class="bricks-wrapper">

            <div class="grid-sizer"></div>

            <div class="brick entry featured-grid animate-this">
                <div class="entry-content">
                    <div id="featured-post-slider" class="flexslider">
                        <ul class="slides">
                            @{
                                IEnumerable<IPublishedContent> typedMultiNodeTreePicker;
                                typedMultiNodeTreePicker = Model.Content.GetPropertyValue<IEnumerable<IPublishedContent>>("slidesPicker");
                                var a = 1;
                            }

                            @foreach (var item in typedMultiNodeTreePicker)
                            {
                                var linkofslide = Umbraco.TypedContent(item.GetPropertyValue("linktoPageofSlide").ToString()).Url;
                            <li>
                                <div class="featured-post-slide">

                                    <div class="post-background" style="background-image:url('@Umbraco.TypedMedia(@item.GetPropertyValue("featureSlidePicture").ToString()).Url');"></div>

                                    <div class="overlay"></div>

                                    <div class="post-content">
                                        <ul class="entry-meta">
                                            <li><a href="@linkofslide">@item.GetPropertyValue("smallText")</a></li>
                                        </ul>

                                        <h1 class="slide-title"><a href="@linkofslide" title="">@item.GetPropertyValue("mainTextOverImage")</a></h1>
                                    </div>

                                </div>
                            </li> <!-- /slide -->
                            }
                        </ul> <!-- end slides -->
                    </div> <!-- end featured-post-slider -->
                </div> <!-- end entry content -->
            </div> <!--1-->

            <article class="brick entry format-standard animate-this">
                @{
                    var linktoAvi = Umbraco.TypedContent(Model.Content.GetPropertyValue("linkfromAviImage").ToString()).Url;
                }
