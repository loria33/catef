                    <a href="@linktoAvi" class="thumb-link">
                        <img src="@Umbraco.TypedMedia(Model.Content.GetPropertyValue("imageofavi").ToString()).Url" alt="אודות">
                    </a>
                </div>

                <div class="entry-text">
                    <div class="entry-header">

                        <div class="entry-meta">
                            <span class="cat-links">
                                <a href="#">ד"ר אבי שזר</a>
                            </span>
                        </div>

                        <h2 class="entry-title">@Model.Content.GetPropertyValue("aviIntro")</h2>

                    </div>
                    <div class="entry-excerpt">
                       @Model.Content.GetPropertyValue("aviIntroMore")
